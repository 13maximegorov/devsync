{
  "name": "devsync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prisma:generate:dev": "npx dotenv -e .env.development -- prisma generate",
    "db:push:dev": "npx dotenv -e .env.development -- prisma db push",
    "migrate:reset:dev": "npx dotenv -e .env.development -- prisma migrate reset",
    "prisma:studio:dev": "npx dotenv -e .env.development -- prisma studio",
    "prisma:generate:prod": "npx dotenv -e .env.production -- prisma generate",
    "db:push:prod": "npx dotenv -e .env.production -- prisma db push",
    "migrate:reset:prod": "npx dotenv -e .env.production -- prisma migrate reset",
    "prisma:studio:prod": "npx dotenv -e .env.production -- prisma studio",
    "build:prisma:prod": "npm run prisma:generate:prod && npm run build",
    "start:prod": "npm run db:push:prod && node server.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.4.0",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@hookform/resolvers": "^3.3.2",
    "@livekit/components-react": "^1.5.3",
    "@livekit/components-styles": "^1.0.9",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "date-fns": "^3.2.0",
    "emoji-mart": "^5.5.2",
    "livekit-server-sdk": "^1.2.7",
    "lucide-react": "^0.292.0",
    "next": "^14.0.4",
    "next-auth": "^5.0.0-beta.15",
    "next-s3-upload": "^0.3.4",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.9",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.49.2",
    "react-type-animation": "^3.2.0",
    "socket.io": "^4.7.3",
